{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalDotEnv": [
    ".env"
  ],
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        "public/dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": []
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "initdb"
      ],
      "dotEnv": [
        ".env"
      ]
    },
    "clean": {
      "cache": false
    },
    "initdb": {
      "cache": false
    },
    "migrate:up": {
      "cache": false
    },
    "api": {
      "cache": false,
      "dependsOn": [
        "initdb"
      ],
      "dotEnv": [
        ".env"
      ]
    },
    "cron": {
      "cache": false,
      "dependsOn": [
        "initdb"
      ],
      "dotEnv": [
        ".env"
      ]
    },
    "client:dev": {
      "cache": false,
      "dotEnv": [
        ".env"
      ]
    },
    "docs:build": {
      "cache": true
    }
  }
}